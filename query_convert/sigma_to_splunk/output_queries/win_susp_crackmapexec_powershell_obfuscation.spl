index=* sourcetype="WinEventLog:Security" EventCode=4688
| search Process_Command_Line="*powershell.exe*" | search Process_Command_Line IN ("*join*split*", "*( $ShellId[1]+$ShellId[13]+'x'")*" OR "*( $PSHome[*]+$PSHOME[*]+*" OR "*( $env:Public[13]+$env:Public[5]+'x')*" OR "*( $env:ComSpec[4,*,25]-Join'')*" OR "*[1,3]+'x'-Join'')*")
| table _time, New_Process_Name, Process_Command_Line