index=* sourcetype="WinEventLog:Security" EventCode=4688
 | search (New_Process_Name="*\RemoteKrbRelay.exe" OR Original_File_Name="RemoteKrbRelay.exe") | search ((Process_Command_Line=" -clsid " OR Process_Command_Line=" -target " OR Process_Command_Line=" -victim ")) | search ((Process_Command_Line="-smb " OR Process_Command_Line="--smbkeyword ") AND (Process_Command_Line="*interactive*" OR Process_Command_Line="*secrets*" OR Process_Command_Line="*service-add*")) | search (Process_Command_Line="*-rbcd *") | search ((Process_Command_Line="*-cn *" OR Process_Command_Line="*--computername *")) | search (Process_Command_Line="*-chp *" AND (Process_Command_Line="-chpPass " OR Process_Command_Line="-chpUser ")) | search ((Process_Command_Line="-addgroupmember " OR Process_Command_Line="-group " OR Process_Command_Line="-groupuser "))
| table _time, New_Process_Name, Process_Command_Line