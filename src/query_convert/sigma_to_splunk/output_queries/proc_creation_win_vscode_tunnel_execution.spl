index=* sourcetype="WinEventLog:Security" EventCode=4688
 | search (Original_File_Name="None" AND Process_Command_Line="*.exe tunnel") OR ((Process_Command_Line=".exe tunnel" OR Process_Command_Line="--name " OR Process_Command_Line="--accept-server-license-terms")) OR (Parent_Process_Command_Line="* tunnel" AND New_Process_Name="*\cmd.exe" AND (Process_Command_Line="/d /c " OR Process_Command_Line="\servers\Stable-" OR Process_Command_Line="code-server.cmd"))
| table _time, New_Process_Name, Process_Command_Line