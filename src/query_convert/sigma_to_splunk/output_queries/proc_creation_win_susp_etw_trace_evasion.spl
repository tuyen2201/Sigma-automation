index=* sourcetype="WinEventLog:Security" EventCode=4688
 | search ((Process_Command_Line="cl" OR Process_Command_Line="/Trace")) OR ((Process_Command_Line="clear-log" OR Process_Command_Line="/Trace")) OR ((Process_Command_Line="sl" OR Process_Command_Line="/e:false")) OR ((Process_Command_Line="set-log" OR Process_Command_Line="/e:false")) OR ((Process_Command_Line="logman" OR Process_Command_Line="update" OR Process_Command_Line="trace" OR Process_Command_Line="--p" OR Process_Command_Line="-ets")) OR (Process_Command_Line="*Remove-EtwTraceProvider*") OR ((Process_Command_Line="Set-EtwTraceProvider" OR Process_Command_Line="0x11"))
| table _time, New_Process_Name, Process_Command_Line