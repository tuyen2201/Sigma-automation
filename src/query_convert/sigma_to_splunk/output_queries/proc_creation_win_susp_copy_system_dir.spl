index=* sourcetype="WinEventLog:Security" EventCode=4688
 | search (New_Process_Name="*\cmd.exe" AND Process_Command_Line="*copy *") OR ((New_Process_Name="*\powershell.exe" OR New_Process_Name="*\pwsh.exe") AND (Process_Command_Line="*copy-item*" OR Process_Command_Line="* copy *" OR Process_Command_Line="*cpi *" OR Process_Command_Line="* cp *")) OR (New_Process_Name="*['\\robocopy.exe', '\\xcopy.exe']" OR Original_File_Name="['robocopy.exe', 'XCOPY.EXE']") OR ((Process_Command_Line="*\System32*" OR Process_Command_Line="*\SysWOW64*" OR Process_Command_Line="*\WinSxS*"))
| table _time, New_Process_Name, Process_Command_Line